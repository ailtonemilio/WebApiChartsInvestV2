@{
    ViewData["Title"] = "Change Password Confirmation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="subheader">
    <h1 class="subheader-title">
        <i class='subheader-icon fal fa-sensor'></i> Web Chart Invest <span class='fw-300'>Change Password Confirmation</span>
    </h1>
    <div class="subheader-block d-lg-flex align-items-center border-faded border-right-0 border-top-0 border-bottom-0 ml-3 pl-3">
        <div class="d-inline-flex flex-column justify-content-center mr-3">
            <div class="position-absolute pos-top pos-right d-none d-sm-block"><span class="js-get-date"></span></div>
        </div>
        <span class="sparklines hidden-lg-down" sparkType="bar" sparkBarColor="#fe6bb0" sparkHeight="32px" sparkBarWidth="5px" values="1,4,3,6,5,3,9,6,5,9,7"></span>
    </div>
</div>

<div id="panel-6" class="panel">
    <div class="panel-hdr bg-primary-700 bg-success-gradient">
        <h2>
            Change Password Confirmed
        </h2>
    </div>
    <div class="panel-container show">
        <div class="panel-content">
            <div class="panel-tag">
                <d1>Your password has been changed successfully!</d1>
            </div>
        </div>
    </div>
</div>